<view>
  <swiper class="swiper_box" circular='ture' indicator-dots="{{indicatorDots}}"                   vertical="{{vertical}}" autoplay="{{autoplay}}" interval="{{interval}}" 
          duration="{{duration}}" bindchange="swiperchange">
    <block wx:for="{{imgUrls}}">
      <swiper-item >
        <image src="{{item.url}}" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>
</view> 