<!--pages/searchPatent/searchPatent.wxml-->

  <view class='header'>
    <view class='searchtype'>标题</view>
    <input class='searchinput' placeholder='请输入搜索关键词'></input>
  </view>
  <view class='searchinfo'>
    <text class='searchnumber'>共检索到0条结果</text>
    <view class='searchdate'>时间排序</view>
    <view class='searchiconview'>
      <image class='searchicon' src='/image/icon/timeUp.png' mode='aspectFit'>
      </image>
    </view>
  </view>
  <view class="item-top-line"></view>
<view wx:for-items="{{msgList}}">
<view class='item'>
<text class="item-title">{{item.title}}</text>
<view class='item-address-view'>
<view class="item-address">{{item.address}}</view>
<view class="item-time">{{item.time}}</view>
</view> 
<view class="item-line"></view>
</view> 
 
</view> 