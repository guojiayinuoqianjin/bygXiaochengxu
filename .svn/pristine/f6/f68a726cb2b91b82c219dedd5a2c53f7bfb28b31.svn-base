    "sourceUrl": "http://www.gdsafety.gov.cn/gdsafety/tongzhi/201711/64fe6fd183e54d3eb4ee54c50918c20e.shtml",
    "md5Url": "e0dc4139e4cb6786f56959dbfb31dd03",
    "isWebsite": "0",
    "downloadUrl": "",
    "addr": "广东省",
    "pubDate": "2017-11-30",
    "pubOrg": null,
    "sourceType": "ZFBM02-33",
