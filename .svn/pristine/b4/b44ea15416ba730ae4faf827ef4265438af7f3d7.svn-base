/* pages/searchTrademark/searchTrademark.wxss */

.searchBgView {
  height: 34px;
}

.searchBox {
  background-color: peru;
  width: 94%;
  height: 30px;
  margin-top: 4px;
  margin-left: 3%;
  padding-left: 4px;
  border-radius: 4px;
  font-size: 14px;
  color: #666;
}

.itemView {
   background-color: white; 
  height: 130px;
  border-top: 1px solid #e8e8e8;
}

.leftBg {
  float: left;
  width: 80px;
  /* background-color: red; */
  height: 100%;
}

.trademarkImage {
  width: 60px;
  height: 60px;
  margin-top: 27px;
  margin-left: 15px;
}

.contentBg {
  position: absolute;
  left: 80px;
  right: 20px;
  /* background-color: gainsboro; */
  height: 130px;
  width: auto;
}

.nameView {
  font-size: 14px;
  color: #666;
  margin-top: 16px;
}

.applicantView {
  margin-top: 8px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.timeView {
  margin-top: 5px;
}

.defaultText {
  font-size: 12px;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.rightBg {
  float: right;
  width: 20px;
  /* background-color: yellow; */
  height: 100%;
  justify-content:center;
  align-items: center;
  display: flex;
}
.moreIcon{
  width: 7.5px;
  height: 14px;
  min-width: 7.5px;
  
}