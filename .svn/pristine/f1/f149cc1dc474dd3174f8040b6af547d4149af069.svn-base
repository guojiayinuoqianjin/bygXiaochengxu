/* pages/searchTrademark/searchTrademark.wxss */

page {
  background-color: #f2f2f2;
}

.searchBgView {
  height: 65px;
  background-color: white;
}

.totalView {
  margin-top: 4px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  margin-left: 3%;
  width: 94%;
}

.searchBox {
  background-color: #f2f2f2;
  width: 94%;
  height: 30px;
  top: 4px;
  margin-left: 3%;
  padding-left: 4px;
  border-radius: 4px;
  font-size: 14px;
  color: #666;
  position: relative;
}

.itemView {
  background-color: white;
  height: 130px;
  border-top: 1px solid #e8e8e8;
}

.leftBg {
  float: left;
  width: 80px;
  /* background-color: red; */
  height: 100%;
}

.trademarkImage {
  width: 60px;
  height: 60px;
  margin-top: 27px;
  margin-left: 15px;
}

.contentBg {
  position: absolute;
  left: 80px;
  right: 20px;
  /* background-color: gainsboro; */
  height: 130px;
  width: auto;
}

.nameView {
  font-size: 14px;
  color: #666;
  margin-top: 16px;
}

.applicantView {
  margin-top: 8px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.timeView {
  margin-top: 5px;
}

.defaultText {
  font-size: 12px;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.rightBg {
  float: right;
  width: 20px;
  /* background-color: yellow; */
  height: 100%;
  justify-content: center;
  align-items: center;
  display: flex;
}

.moreIcon {
  width: 7.5px;
  height: 14px;
  min-width: 7.5px;
}

.loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
