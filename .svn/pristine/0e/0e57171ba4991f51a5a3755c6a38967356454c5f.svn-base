/* pages/handleTrademarkDetails/handleTrademarkDetails.wxss */

.bgView {
  width: 100%;
  height: 50px;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.bgView view {
  font-size: 12px;
  color: #666;
  width: 90px;
  padding-left: 5px;
}

.bgView view text {
  color: #fe6246;
}

.bgView input {
  font-size: 12px;
  color: #666;
  /* background-color: red; */
  width: 80%;
  display: flex;
}

/* 左  右上下  */

.bgView2 {
  width: 100%;
  height: 100px;
  background-color: white;
  margin-top: 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.applyType {
  font-size: 12px;
  color: #666;
  width: 90px;
  padding-left: 5px;
}

.applyType text {
  color: #fe6246;
}

.typeBgView{
  width: 100%;
  background-color: white;
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
}
.typeItem{
  width:33.33%;
  height: 30px;
  line-height: 30px;
  font-size: 10px;
  text-align: center;
}
.typeItemSel{
  color: #fe6246;
}
.typeItemNor{
  color: #666;
}

.rightBgView {
  width: 80%;
  /* background-color: red; */
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.itemView {
  font-size: 12px;
  width: 60%;
  height: 30px;
  margin-top: 5px;
  margin-bottom: 5px;
  margin-left: 20px;
  line-height: 30px;
  text-align: center;
}
.selTypeItem {
  border: 1px dashed #fe6246;
  color: #fe6246;
}

.normalTypeItem {
  border: 1px dashed #ccc;
  color: #666;
}

.bgView2 image {
  width: 75px;
  height: 75px;
  border: 1px dashed #ccc;
}

.selBgView {
  background-color: white;
  margin-top: 10px;
  margin-bottom: 40px;
  min-height: 30px;
  padding: 10px 0;
}

.picker {
  border: 1px dashed #ccc;
  height: 30px;
  width: 80%;
  margin-top: 5px;
  margin-bottom: 5px;
  margin-left: 20px;
  line-height: 30px;
  font-size: 12px;
  color: #666;
}
.selTypeItemView{
  /* margin-top: 5px; */
  color: #666;
  font-size: 12px;
  padding-top: 5px;
  height: 20px;
  padding-left: 20px;
  border-bottom: 1px dashed #ccc;
}
.bottomBtnView {
  width: 100%;
  height: 44px;
  background-color: #fe6246;
  position: fixed;
  bottom: 0;
  left: 0;
  color: white;
  text-align: center;
  line-height: 44px;
}

.phoneInputBgView {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: black;
  left: 0;
  top: 0;
  opacity: 0.5;
  z-index: 5;
}

.phoneInputView {
  width: 80%;
  height: 180px;
  background-color: white;
  opacity: 1;
  z-index: 6;
  left: 10%;
  top: 100px;
  position: fixed;
  border-radius: 5px;
}

.submitTitle {
  width: 100%;
  text-align: center;
  color: #666;
  font-size: 16px;
  margin-top: 5px;
}

.imputTitle {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 10px;
  margin-top: 10px;
  font-size: 14px;
  color: #666;
  height: 30px;
  line-height: 30px;
  align-items: center;
}

.imputTitle span {
  color: #fe6246;
}

.imputTitle input {
  border: 1px dashed #ccc;
  color: #666;
  width: 70%;
  margin-right: 20px;
  padding-left: 5px;
}

.submitBgView {
  width: 100%;
  height: 34px;
  /* background-color:yellow; */
  margin-top: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.submitView {
  width: 200px;
  height: 34px;
  background-color: #fe6246;
  color: white;
  border-radius: 5px;
  text-align: center;
  line-height: 34px;
  font-size: 18px;
}