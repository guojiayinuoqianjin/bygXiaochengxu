/* pages/policy/policy.wxss */

/* 头部背景图 */
.page{
  z-index: 3;
}
.header {
  height: 74px;
  background-color: white;
  padding-top: 5px;
  box-sizing: border-box;
 
}

.subView {
  width: 92%;
  height: 34px;
  border-radius: 4px;
  background-color: white;
  margin: auto;
  line-height: 34px;
  overflow: hidden;
  color: white;
  display: flex;  
}

.typeView {
  height: 100%;
  flex: 0 0 40px;
  line-height: 34px;
  font-size: 14px;
  padding: 0;
  background-color: #fe6246;
  float: left;
  display: flex;
  align-items: center;
  justify-content: center;
}

.search {
  background-color: #f2f2f2;
  height: 100%;
  /* float: right; */
  color: #666;
  display: flex;
  /*弹性缩放  */
  flex: 1;
  font-size: 14px;
  padding-left: 5px;
  /* font-family: "黑体"; */
}

.totalLab {
  width: 50%;
   /* background-color: orange;  */
  height: 35px;
  line-height: 35px;
  margin-left: 4%;
  font-size: 14px;
  color: #666;
  float: left;
}

.sortIcon {
  width: 17.5px;
  height: 16.5px;
  float: right;
  /* background-color: yellow; */
  margin-top: 9px;
  /* display: flex; */
}

.timeBg {
   float: right; 
  width: 75.5px;
  height: 35px;
   /* margin-right: 10px; */
  /* background-color: orange;  */
  /* position:relative; */
}

.timeSort {
  width: 58px;
   float: left; 
  height: 35px;
  line-height: 35px;
  font-size: 14px;
  color: #666;
   
}
.filterIcon{
  font-size: 14px;
  width: 40px;
   float: right; 
  /* background-color: red; */
   margin-right: 4%; 
  height: 35px;
  line-height: 35px;
  text-align: right;
  color: #fe6246;
}
.itemView {
  height: 90px;
  width: 100%;
  background-color: white;
  border-top: 1px solid #e8e8e8;
  overflow: hidden;
}

.titleView {
  display: -webkit-box;
  width: 92%;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 14px;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  /* background-color: yellow; */
  height: 50px;
  line-height: 25px;
  margin: auto;
  margin-top: 10px;
  color: #333;
}

.addrView {
  float: left;
  margin-left: 4%;
  /* background-color: red; */
}

.timeView {
  float: right;
  margin-right: 4%;
  /* background-color: green; */
  text-align: right;
}

.subtext {
  width: 46%;
  height: 30px;
  color: #999;
  font-size: 12px;
  line-height: 30px;
}

.loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

.promptView{
  color: #fe6246;
  width: 50%;
  /* background-color: yellow; */
  height: 34px;
  text-align: center;
  margin: 0 auto;  
  line-height: 34px;
}
.otherInfoPrompt{
  color: #fe6246;
  width: 100%;
  height: 34px;
  line-height: 34px;
  font-size: 14px;
  text-align: center;
}
.filterView {
  background-color: white;
  position: fixed;
  z-index: 20;
  width: 100%;
  height: 100%;
  top: 0px;
  overflow: hidden;
  display: flex;
  flex-flow: column;
}

.selFilter {
  color: #fe6246;
}

.normal {
  color: #666;
}

/*筛选头  */

.filterHeader {
  width: 100%;
  /* background-color: red; */
  height: 102px;
}

/*筛选内容选择  */

.contentView {
  width: 100%;
  /* background-color: yellow; */
  padding: 0 0 40px 0;
  min-height: 300px;
  flex: 1;
  flex-flow: column;
}

/*筛选尾  */

.filterBottomView {
  width: 100%;
  /* background-color: green; */
  height: 40px;
  position: fixed;
  bottom: 0;
}

.closeImage {
  width: 34px;
  height: 34px;
  /* background-color: yellowgreen; */
  display: flex;
  float: left;
}

.chooseResultBgView {
  width: 100%;
  height: 34px;
  /* background-color: yellowgreen;  */
  display: flex;
  border-bottom: 1px solid #f2f2f2;
}

.provinceView {
  width: 25%;
  height: 100%;
  /* background-color: gold; *//* display: flex;    加它尺寸自适应*//* background: pink; */
  overflow: hidden;
  float: left;
}

.filterItemView {
  height: 54px;
  width: 100%;
  /* background-color: red; */
  border-top: 1px solid #fff;
  font-size: 12px;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
}

.titleItem {
  width: 25%;
  height: 100%;
  text-align: center;
  line-height: 34px;
}

.sureView {
  width: 100%;
  height: 40px;
  line-height: 40px;
  /* margin-bottom: ; */
  background-color: #fe6246;
  /* position: fixed; */
  text-align: center;
  color: white;
}

/*筛选结果展示项  */

.selItem {
  float: left;
  line-height: 34px;
  display: flex;
  font-size: 12px;
  /* background-color: red; */
  align-items: center;
  justify-content: center;
  padding: 0 10px;
}
